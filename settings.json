{
  "window.zoomLevel": -2,
  "workbench.welcome.enabled": false,
  "editor.fontSize": 12,
  "editor.cursorStyle": "block",
  "editor.cursorBlinking": "solid",
  "editor.tabSize": 2,
  // "workbench.panel.defaultLocation": "bottom",
  "editor.formatOnSave": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "extensions.ignoreRecommendations": false,
  "eslint.run": "onType",
  "[elm]": {
    "editor.formatOnSave": true
    // "elm.makeCommand": "./node_modules/.bin/elm-make"
  },
  "elm.makeCommand": "elm make",
  // "prettier.semi": false
  // "flowide.enable": true,
  // "extensions.ignoreRecommendations": false
  // "java.errors.incompleteClasspath.severity": "ignore",
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "postcssSorting.config": {
    "order": [
      "custom-properties",
      "dollar-variables",
      "declarations",
      "at-rules",
      "rules"
    ],
    "properties-order": [
      "position",
      "z-index",
      "top",
      "right",
      "bottom",
      "left",
      "display",
      "flex-direction",
      "flex",
      "width",
      "max-width",
      "min-width",
      "height",
      "margin",
      "margin-top",
      "margin-right",
      "margin-bottom",
      "margin-left",
      "padding",
      "padding-top",
      "padding-right",
      "padding-bottom",
      "padding-left",
      "text-align",
      "transform",
      "border",
      "border-radius",
      "background-color",
      "box-shadow",
      "align-items",
      "border-width",
      "cursor",
      "justify-content",
      "line-height",
      "cursor",
      "color",
      "user-select",
      "text-transform",
      "font-size",
      "font-weight",
      "line-height"
    ]
  },
  "terminal.integrated.fontSize": 12,
  "window.openFilesInNewWindow": "on",
  "workbench.colorTheme": "Material Theme Darker High Contrast",
  "workbench.iconTheme": "eq-material-theme-icons-darker",
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "emojisense.languages": {
    "markdown": true,
    "git-commit": false,
    "plaintext": false,
    "json": true,
    "javascript": true,
    "javascriptreact": true,
    "typescript": true,
    "typescriptreact": true
  },
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ]
}
